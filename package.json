{
  "name": "@travi/eslint-config-mocha",
  "description": "Shareable config for ESLint for Mocha tests",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "travi/eslint-config-mocha",
  "bugs": "https://github.com/travi/eslint-config-mocha/issues",
  "homepage": "https://npm.im/@travi/eslint-config-mocha",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint .",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml"
  },
  "devDependencies": {
    "@travi/eslint-config": "1.0.1",
    "ban-sensitive-files": "^1.9.2",
    "commitlint-config-travi": "^1.3.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-travi": "^1.2.0",
    "travis-lint": "^1.0.0"
  },
  "dependencies": {
    "@form8ion/eslint-config-mocha": "1.2.0"
  }
}
